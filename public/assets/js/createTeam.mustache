<div class="modal fade" id="createTeam">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title"><strong>New Team</strong></h4>
			</div>
			<div class="modal-body">
				<div class="input-group">
					<span class="input-group-btn">
						<button class="btn btn-default" type="button">Name:</button>
					</span>
					<input type="text" class="form-control" can-value='createdTeam.teamName'>
					</div><!-- /input-group -->
					<div class="input-group">
						<span class="input-group-btn">
							<button class="btn btn-default" type="button">Slogan:</button>
						</span>
						<input type="text" class="form-control" can-value='createdTeam.slogan'>
						</div><!-- /input-group -->
						<div class="input-group">
							<span class="input-group-btn">
								<button class="btn btn-default" type="button">Wins:</button>
							</span>
							<input type="text" class="form-control" can-value='createdTeam.wins'>
							</div><!-- /input-group -->
							<div class="input-group">
								<span class="input-group-btn">
									<button class="btn btn-default" type="button">Losses:</button>
								</span>
								<input type="text" class="form-control" can-value='createdTeam.losses'>
								</div><!-- /input-group -->
								<div class="input-group">
									<span class="input-group-btn">
										<button class="btn btn-default" type="button">Hobbies:</button>
									</span>
									<input type="text" class="form-control" can-value='createdTeam.hobbies'>
									</div><!-- /input-group -->
									<div class="input-group">
										<span class="input-group-btn">
											<button class="btn btn-default" type="button">Members:</button>
										</span>
										<input type="text" class="form-control" can-value='createdTeam.stringMembers'>
										</div><!-- /input-group -->
									</div>
									<div class="modal-footer">
										<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
										<button type="button" class="btn btn-primary" can-click='createTeam'>Save changes</button>
									</div>
									</div><!-- /.modal-content -->
									</div><!-- /.modal-dialog -->
									</div><!-- /.modal -->